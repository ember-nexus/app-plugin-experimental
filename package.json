  "browserslist": ["last 2 years"],
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && yarn build:esm && yarn build:browser",
    "build:browser": "vite build",
    "build:esm": "yarn tsc -b tsconfig.esm.json && yarn cpy 'src/**/*' '!src/**/*.ts' dist/esm",
    "cs": "eslint ./src",
    "cs:fix": "eslint ./src --fix",
    "test:unit": "vitest run ./test/Unit",
    "test:unit:coverage": "vitest run ./test/Unit --coverage",
    "prepare": "ts-patch install",
    "typedoc": "typedoc --tsconfig tsconfig.release.json",
    "storybook": "storybook dev -p 6006 -h 0.0.0.0",
    "build-storybook": "storybook build"
  },
