{
  "name": "@ember-nexus/app-plugin-experimental",
  "version": "0.0.2",
  "type": "module",
  "engines": {
    "node": ">= 20.0"
  },
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && yarn build:esm && yarn build:browser",
    "build:browser": "webpack --config webpack.config.cjs",
    "build:esm": "yarn tsc -b tsconfig.esm.json",
    "cs": "eslint ./src",
    "cs:fix": "eslint ./src --fix",
    "test:unit": "jest ./test/Unit",
    "test:feature": "jest ./test/Feature",
    "test:example": "yarn build && jest ./test/Example",
    "prepare": "ts-patch install",
    "typedoc": "typedoc --tsconfig tsconfig.release.json"
  },
  "author": "Syndesi <info@ember-nexus.dev>",
  "license": "MIT",
  "dependencies": {
    "@ember-nexus/web-sdk": "^0.0.59",
    "apca-w3": "^0.1.9",
    "color-hash": "^2.0.2",
    "colorparsley": "^0.1.8",
    "lit": "^3.1.3",
    "tslog": "^4.9.2",
    "xstate": "^5.13.1"
  },
  "devDependencies": {
